{
    "manifest_version": 3,
    "name": "GSRextend3 v3.9.1",
    "version": "3.9.1",
    "description": "Adds result stats text as a new div on Google search page.",
    "content_scripts": [
        {
            "matches": ["https://www.google.com/*"],
            "js": ["content.js"],
            "css": ["styles.css"],
            "run_at": "document_idle"
        }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/icon48.png"
    }
}
